{
  "manifest_version": 3,
  "name": "Enter Alchemist",
  "version": "1.0",
  "description": "Convert Enter key press to Enter behavior",
  "permissions": ["storage", "activeTab"],
  "content_scripts": [
    {
      "matches": ["*://apps.abacus.ai/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
